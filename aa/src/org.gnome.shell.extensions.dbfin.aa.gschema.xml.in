<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="gnome-shell-extension-aa">
  <schema id="org.gnome.shell.extensions.dbfin.aa" path="/org/gnome/shell/extensions/dbfin/aa/">
    <key name="first-time" type="b">
      <default>true</default>
      <_summary>Whether the extension is run for the first time.</_summary>
      <_description>Whether the extension is run for the first time.</_description>
    </key>
    <key name="advanced" type="b">
      <default>false</default>
      <_summary>Whether to show advanced preferences.</_summary>
      <_description>Whether to show advanced preferences.</_description>
    </key>
    <key name="extension-frequencies" type="s">
      <default>'[]'</default>
      <_summary>Extension frequency of access.</_summary>
      <_description>Extension frequency of access.</_description>
    </key>
    <key name="extension-favorites" type="s">
      <default>'[]'</default>
      <_summary>Favorite extensions.</_summary>
      <_description>Favorite extensions.</_description>
    </key>
    <key name="style-force-default" type="b">
      <default>true</default>
      <_summary>Whether to force default panel button style.</_summary>
      <_description>Whether to force default panel button style.</_description>
    </key>
    <key name="style-background" type="s">
      <default>'1'</default>
      <_summary>Activities button background.</_summary>
      <_description>Activities button background.</_description>
    </key>
    <key name="style-custom-css" type="s">
      <default>'~/.local/share/gnome-shell/extensions/aa@dbfin.com/stylesheet_custom.css'</default>
      <_summary>Custom css file.</_summary>
      <_description>Custom css file.</_description>
    </key>
    <key name="submenu-extension-manager" type="b">
      <default>true</default>
      <_summary>Whether to show extensions in the menu.</_summary>
      <_description>Whether to show extensions in the menu.</_description>
    </key>
    <key name="extension-manager-sort" type="s">
      <default>'2'</default>
      <_summary>Extension manager sort method.</_summary>
      <_description>Extension manager sort method.</_description>
    </key>
    <key name="extension-manager-show-favorites" type="b">
      <default>false</default>
      <_summary>Show options for favorite extensions only.</_summary>
      <_description>Show options for favorite extensions only.</_description>
    </key>
    <key name="submenu-additional" type="b">
      <default>true</default>
      <_summary>Whether to show additional commands in the menu.</_summary>
      <_description>Whether to show additional commands in the menu.</_description>
    </key>
    <key name="mouse-scroll-timeout" type="s">
      <default>'125'</default>
      <_summary>Mouse single scroll time.</_summary>
      <_description>Mouse single scroll time (to prevent multiple scrolls).</_description>
    </key>
  </schema>
</schemalist>
