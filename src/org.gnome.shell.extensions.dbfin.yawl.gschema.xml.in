<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="gnome-shell-extension-yawl">
  <schema id="org.gnome.shell.extensions.dbfin.yawl" path="/org/gnome/shell/extensions/dbfin/yawl/">
    <key name="first-time" type="b">
      <default>true</default>
      <_summary>Whether the extension is run for the first time.</_summary>
      <_description>Whether the extension is run for the first time.</_description>
    </key>
    <key name="icons-show-all" type="b">
      <default>true</default>
      <_summary>Whether to show icons from all workspaces.</_summary>
      <_description>If true icons form all workspaces are shown.</_description>
    </key>
    <key name="yawl-panel-position" type="s">
      <default>'21'</default>
      <_summary>YAWL-panel position in percentage.</_summary>
      <_description>The position of the left side of the YAWL panel as percentage of the top panel width.</_description>
    </key>
    <key name="yawl-panel-width" type="s">
      <default>'100'</default>
      <_summary>YAWL-panel width in percentage.</_summary>
      <_description>The width of the YAWL panel as percentage of the top panel width.</_description>
    </key>
    <key name="move-center" type="b">
      <default>true</default>
      <_summary>Move central panel to the right.</_summary>
      <_description>If true the central panel is moved to the right.</_description>
    </key>
    <key name="hide-activities" type="b">
      <default>false</default>
      <_summary>Hide Activities button.</_summary>
      <_description>If true the Activities button is hidden.</_description>
    </key>
    <key name="preserve-hot-corner" type="b">
      <default>true</default>
      <_summary>Preserve Hot Corner.</_summary>
      <_description>If true the Hot Corner is available even if the Activities button is hidden.</_description>
    </key>
    <key name="hide-app-menu" type="b">
      <default>false</default>
      <_summary>Hide AppMenu button.</_summary>
      <_description>If true the AppMenu button is hidden.</_description>
    </key>
    <key name="panel-background" type="b">
      <default>false</default>
      <_summary>Custom panel background.</_summary>
      <_description>If true the top panel is painted with custom color and opacity.</_description>
    </key>
    <key name="panel-top-color" type="s">
      <default>'#212133'</default>
      <_summary>Choose custom panel top color.</_summary>
      <_description>Custom color to paint top of the panel.</_description>
    </key>
    <key name="panel-top-opacity" type="s">
      <default>'75'</default>
      <_summary>Choose custom panel top opacity.</_summary>
      <_description>Custom opacity to paint top of the panel.</_description>
    </key>
    <key name="panel-color" type="s">
      <default>'#000000'</default>
      <_summary>Choose custom panel color.</_summary>
      <_description>Custom color to paint the panel.</_description>
    </key>
    <key name="panel-opacity" type="s">
      <default>'75'</default>
      <_summary>Choose custom panel opacity.</_summary>
      <_description>Custom opacity to paint the panel.</_description>
    </key>

    <key name="icons-size" type="s">
      <default>'24'</default>
      <_summary>Size of application icons.</_summary>
      <_description>The size of application icons. The size is adjusted to be divisible by 8.</_description>
    </key>
    <key name="icons-faded" type="b">
      <default>false</default>
      <_summary>Use faded icons.</_summary>
      <_description>Whether to use faded icons (like AppMenu button).</_description>
    </key>
    <key name="icons-opacity" type="s">
      <default>'84'</default>
      <_summary>Default icons opacity.</_summary>
      <_description>Icon opacity when not hovered (and when hovered if not otherwise set).</_description>
    </key>
    <key name="icons-opacity-inactive" type="s">
      <default>'21'</default>
      <_summary>Opacity of app icons not on current workspace.</_summary>
      <_description>Opacity of app icons not on current workspace.</_description>
    </key>
    <key name="icons-clip-top" type="s">
      <default>'2'</default>
      <_summary>Clip icons: top (px).</_summary>
      <_description>Clip icons on top (in pixels).</_description>
    </key>
    <key name="icons-clip-bottom" type="s">
      <default>'2'</default>
      <_summary>Clip icons: bottom (px).</_summary>
      <_description>Clip icons on bottom (in pixels).</_description>
    </key>
    <key name="icons-align" type="s">
      <default>'0'</default>
      <_summary>Align icons on the panel.</_summary>
      <_description>Icons center alignment on the panel (%).</_description>
