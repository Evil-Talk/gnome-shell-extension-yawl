      <default>'11'</default>
      <_summary>Distance between window thumbnails (%).</_summary>
      <_description>Distance between window thumbnails as % of their widths.</_description>
    </key>
    <key name="windows-thumbnails-padding-top" type="s">
      <default>'7'</default>
      <_summary>Thumbnail top padding (px).</_summary>
      <_description>Thumbnail top padding in pixels.</_description>
    </key>

    <key name="windows-show-delay" type="s">
      <default>'400'</default>
      <_summary>Window thumbnails showing delay (in ms).</_summary>
      <_description>Window thumbnails showing delay time in ms.</_description>
