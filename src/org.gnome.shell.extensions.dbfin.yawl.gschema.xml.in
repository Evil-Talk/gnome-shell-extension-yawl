    <key name="icons-hover-opacity" type="s">
      <default>'100'</default>
      <_summary>Opacity of hovered icon.</_summary>
      <_description>Defines the opacity of the hovered icon.</_description>
    </key>
    <key name="icons-hover-fit" type="b">
      <default>false</default>
      <_summary>Whether to show full icon when hovered.</_summary>
      <_description>If true icons are shown full width when hovered.</_description>
    </key>
    <key name="icons-hover-animation-time" type="s">
      <default>'33'</default>
      <_summary>Icons on hover animation time (%).</_summary>
      <_description>Icons animation time when hovered as % of animation time.</_description>
