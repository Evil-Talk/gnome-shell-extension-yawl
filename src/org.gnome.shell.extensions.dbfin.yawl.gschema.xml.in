<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="gnome-shell-extension-yawl">
  <schema id="org.gnome.shell.extensions.dbfin.yawl" path="/org/gnome/shell/extensions/dbfin/yawl/">
    <key name="yawl-panel-position" type="s">
      <default>'21'</default>
      <_summary>YAWL-panel position in percentage.</_summary>
      <_description>The position of the left side of the YAWL panel as percentage of the top panel width.</_description>
    </key>
    <key name="yawl-panel-width" type="s">
      <default>'100'</default>
      <_summary>YAWL-panel width in percentage.</_summary>
      <_description>The width of the YAWL panel as percentage of the top panel width.</_description>
    </key>
    <key name="move-center" type="b">
      <default>true</default>
      <_summary>Move central panel to the right.</_summary>
      <_description>If true the central panel is moved to the right.</_description>
    </key>
    <key name="hide-activities" type="b">
      <default>true</default>
      <_summary>Hide Activities button.</_summary>
      <_description>If true the Activities button is hidden.</_description>
    </key>
    <key name="preserve-hot-corner" type="b">
      <default>true</default>
      <_summary>Preserve Hot Corner.</_summary>
      <_description>If true the Hot Corner is available even if the Activities button is hidden.</_description>
    </key>
    <key name="hide-app-menu" type="b">
      <default>false</default>
      <_summary>Hide AppMenu button.</_summary>
      <_description>If true the AppMenu button is hidden.</_description>
    </key>
    <key name="panel-background" type="b">
      <default>false</default>
      <_summary>Custom panel background.</_summary>
      <_description>If true the top panel is painted with custom color and opacity.</_description>
    </key>
    <key name="panel-color" type="s">
      <default>'#000000'</default>
      <_summary>Choose custom panel color.</_summary>
      <_description>Custom color to paint the top panel.</_description>
    </key>
    <key name="panel-opacity" type="s">
      <default>'100'</default>
      <_summary>Choose custom panel opacity.</_summary>
      <_description>Custom opacity to paint the top panel.</_description>
    </key>

    <key name="icons-size" type="s">
      <default>'48'</default>
      <_summary>Size of application icons.</_summary>
      <_description>The size of application icons. The size is adjusted to be divisible by 8.</_description>
    </key>
    <key name="icons-faded" type="b">
      <default>true</default>
      <_summary>Use faded icons.</_summary>
      <_description>Whether to use faded icons (like AppMenu button).</_description>
    </key>
    <key name="icons-opacity" type="s">
      <default>'84'</default>
      <_summary>Default icons opacity.</_summary>
      <_description>Icon opacity when not hovered (and when hovered if not otherwise set).</_description>
    </key>
    <key name="icons-clip-top" type="s">
      <default>'3'</default>
      <_summary>Clip icons: top (px).</_summary>
      <_description>Clip icons on top (in pixels).</_description>
    </key>
    <key name="icons-clip-bottom" type="s">
      <default>'3'</default>
      <_summary>Clip icons: bottom (px).</_summary>
      <_description>Clip icons on bottom (in pixels).</_description>
    </key>
    <key name="icons-align" type="s">
      <default>'0'</default>
      <_summary>Align icons on the panel.</_summary>
      <_description>Icons center alignment on the panel (%).</_description>
    </key>
    <key name="icons-distance" type="s">
      <default>'21'</default>
      <_summary>Distance between icons (%).</_summary>
      <_description>Distance between two icons as % of the icon size.</_description>
    </key>

    <key name="icons-animation-time" type="s">
      <default>'490'</default>
      <_summary>Icons animation time.</_summary>
      <_description>Icons animation time in ms. If 0 then no animation is used.</_description>
    </key>
    <key name="icons-animation-effect" type="s">
      <default>'1'</default>
      <_summary>Icons animation effect.</_summary>
      <_description>Icons animation effect.</_description>
    </key>
    <key name="icons-hover-animation" type="b">
      <default>true</default>
      <_summary>Whether to animate icons when hovered.</_summary>
      <_description>If true icons are animated (changing size etc.) when hovered.</_description>
    </key>
    <key name="icons-hover-size" type="s">
      <default>'100'</default>
      <_summary>Size of hovered icon (% relative to unhovered).</_summary>
      <_description>Defines the size of the hovered icon in % relative to the unhovered one.</_description>
    </key>
    <key name="icons-hover-opacity" type="s">
      <default>'100'</default>
      <_summary>Opacity of hovered icon.</_summary>
      <_description>Defines the opacity of the hovered icon.</_description>
    </key>
    <key name="icons-hover-fit" type="b">
      <default>false</default>
      <_summary>Whether to show full icon when hovered.</_summary>
      <_description>If true icons are shown full width when hovered.</_description>
    </key>
    <key name="icons-hover-animation-time" type="s">
      <default>'33'</default>
      <_summary>Icons on hover animation time (%).</_summary>
      <_description>Icons animation time when hovered as % of animation time.</_description>
    </key>
    <key name="icons-hover-animation-effect" type="s">
      <default>'0'</default>
      <_summary>Icons on hover animation effect.</_summary>
      <_description>Icons animation effect when hovered.</_description>
    </key>

    <key name="windows-indicator-arrow" type="b">
      <default>false</default>
      <_summary>Whether to use arrow-style thumbnail application indicator.</_summary>
      <_description>If true the indicator of the current thumbnails application is drawn as an arrow</_description>
    </key>
    <key name="windows-theming" type="b">
      <default>true</default>
      <_summary>Whether to apply thumbnail theming.</_summary>
      <_description>If true thumbnails a drawn using windows-padding etc.</_description>
    </key>
    <key name="windows-background-panel" type="b">
      <default>true</default>
      <_summary>Whether to paint thumbnail panel with main panel background.</_summary>
      <_description>If true thumbnail panel background matches main panel background.</_description>
    </key>
    <key name="windows-background-color" type="s">
      <default>'#000000'</default>
      <_summary>Window thumbnail panel background color.</_summary>
      <_description>Window thumbnail panel background color.</_description>
    </key>
    <key name="windows-background-opacity" type="s">
      <default>'70'</default>
      <_summary>Window thumbnail panel background opacity.</_summary>
      <_description>Window thumbnail panel background background opacity.</_description>
    </key>
    <key name="windows-padding" type="s">
      <default>'7'</default>
      <_summary>Window thumbnail padding.</_summary>
      <_description>Window thumbnail padding.</_description>
    </key>
    <key name="windows-border-color" type="s">
      <default>'#d3d7cf'</default>
      <_summary>Window thumbnail border color.</_summary>
      <_description>Window thumbnail border color.</_description>
    </key>
    <key name="windows-border-width" type="s">
      <default>'2'</default>
      <_summary>Window thumbnail border width.</_summary>
      <_description>Window thumbnail border width.</_description>
    </key>
    <key name="windows-border-radius" type="s">
      <default>'7'</default>
      <_summary>Window thumbnail border radius.</_summary>
      <_description>Window thumbnail border radius.</_description>
    </key>

    <key name="windows-thumbnails-width" type="s">
      <default>'248'</default>
      <_summary>Width of window thumbnails.</_summary>
      <_description>The width of window thumbnails.</_description>
    </key>
    <key name="windows-thumbnails-fit-height" type="b">
      <default>true</default>
      <_summary>Whether to show all thumbnails same height.</_summary>
      <_description>If true then all thumbnails are same height.</_description>
    </key>
    <key name="windows-thumbnails-height" type="s">
      <default>'160'</default>
      <_summary>Height of window thumbnails.</_summary>
      <_description>The height of window thumbnails.</_description>
    </key>
    <key name="windows-thumbnails-height-visible" type="s">
      <default>'160'</default>
      <_summary>Window thumbnail maximum visible height.</_summary>
      <_description>Window thumbnail maximum visible height.</_description>
    </key>
    <key name="windows-thumbnails-opacity" type="s">
      <default>'84'</default>
      <_summary>Default windows opacity.</_summary>
      <_description>Windows opacity when not hovered (and when hovered if not otherwise set).</_description>
    </key>
    <key name="windows-thumbnails-distance" type="s">
      <default>'11'</default>
      <_summary>Distance between window thumbnails (%).</_summary>
      <_description>Distance between window thumbnails as % of their widths.</_description>
    </key>
    <key name="windows-thumbnails-padding-top" type="s">
      <default>'7'</default>
      <_summary>Thumbnail top padding (px).</_summary>
      <_description>Thumbnail top padding in pixels.</_description>
    </key>

    <key name="windows-show-delay" type="s">
      <default>'400'</default>
      <_summary>Window thumbnails showing delay (in ms).</_summary>
      <_description>Window thumbnails showing delay time in ms.</_description>
    </key>
    <key name="windows-animation-time" type="s">
      <default>'400'</default>
      <_summary>Windows animation time.</_summary>
      <_description>Windows animation time in ms. If 0 then no animation is used.</_description>
    </key>
    <key name="windows-animation-effect" type="s">
      <default>'3'</default>
      <_summary>Windows animation effect.</_summary>
      <_description>Windows animation effect.</_description>
    </key>
    <key name="windows-hover-opacity" type="s">
      <default>'100'</default>
      <_summary>Opacity of hovered window thumbnails.</_summary>
      <_description>Defines the opacity of the hovered window thumbnail.</_description>
    </key>
    <key name="windows-hover-fit" type="b">
      <default>true</default>
      <_summary>Whether to show full window thumbnail when hovered.</_summary>
      <_description>If true window thuumbnails are shown full width when hovered.</_description>
    </key>
    <key name="windows-hover-animation-time" type="s">
      <default>'77'</default>
      <_summary>Window thumbnails on hover animation time (%).</_summary>
      <_description>Window thumbnails animation time when hovered as % of animation time.</_description>
    </key>
    <key name="windows-hover-animation-effect" type="s">
      <default>'3'</default>
      <_summary>Window thumbnails on hover animation effect.</_summary>
      <_description>Window thumbnails animation effect when hovered.</_description>
    </key>

    <key name="mouse-app-left" type="s">
      <default>'1'</default>
      <_summary>App left click/double click function.</_summary>
      <_description>App left click/double click function.</_description>
    </key>
    <key name="mouse-app-left-ctrl" type="s">
      <default>'8'</default>
      <_summary>App ctrl-left click/double click function.</_summary>
      <_description>App ctrl-left click/double click function.</_description>
    </key>
    <key name="mouse-app-left-shift" type="s">
      <default>'6'</default>
      <_summary>App shift-left click/double click function.</_summary>
      <_description>App shift-left click/double click function.</_description>
    </key>
    <key name="mouse-app-left-ctrl-shift" type="s">
      <default>'2'</default>
      <_summary>App ctrl-shift-left click/double click function.</_summary>
      <_description>App ctrl-shift-left click/double click function.</_description>
    </key>
    <key name="mouse-long-click" type="b">
      <default>true</default>
      <_summary>Whether to emulate right-click with long left-click.</_summary>
      <_description>If true long mouse left button click emulates mouse right button click.</_description>
    </key>

    <key name="mouse-app-middle" type="s">
      <default>'9'</default>
      <_summary>App middle click/double click function.</_summary>
      <_description>App middle click/double click function.</_description>
    </key>
    <key name="mouse-app-middle-ctrl" type="s">
      <default>'3'</default>
      <_summary>App ctrl-middle click/double click function.</_summary>
      <_description>App ctrl-middle click/double click function.</_description>
    </key>
    <key name="mouse-app-middle-shift" type="s">
      <default>'7'</default>
      <_summary>App shift-middle click/double click function.</_summary>
      <_description>App shift-middle click/double click function.</_description>
    </key>
    <key name="mouse-app-middle-ctrl-shift" type="s">
      <default>'0'</default>
      <_summary>App ctrl-shift-middle click/double click function.</_summary>
      <_description>App ctrl-shift-middle click/double click function.</_description>
    </key>
    <key name="mouse-app-scroll" type="s">
      <default>'4'</default>
      <_summary>App mouse scroll function.</_summary>
      <_description>App mouse scroll function.</_description>
    </key>

    <key name="mouse-app-right" type="s">
      <default>'10'</default>
      <_summary>App right click/double click function.</_summary>
      <_description>App right click/double click function.</_description>
    </key>

    <key name="mouse-click-release" type="b">
      <default>false</default>
      <_summary>Whether to send clicks on mouse release.</_summary>
      <_description>If true mouse clicks are sent when mouse button is released rather than pressed.</_description>
    </key>
    <key name="mouse-clicks-time-single" type="s">
      <default>'400'</default>
      <_summary>Single clicks time.</_summary>
      <_description>Single clicks time.</_description>
    </key>
    <key name="mouse-clicks-time-double" type="s">
      <default>'250'</default>
      <_summary>Double clicks time.</_summary>
      <_description>Double clicks time.</_description>
    </key>
    <key name="mouse-clicks-time-threshold" type="s">
      <default>'300'</default>
      <_summary>Mouse single/double clicks time threshold.</_summary>
      <_description>Mouse single/double clicks time threshold.</_description>
    </key>
  </schema>
</schemalist>
